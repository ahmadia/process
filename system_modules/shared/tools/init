#%Module
#
# Initialization module
#
# KAUST Supercomputing Laboratories
# shaheen-help@kaust.edu.sa

module-whatis "environment initialization (resets user build environment flags)"

proc ModulesHelp { } {
    puts stderr \
"this module clears the user's build environment and is loaded first
"
}

setenv CFLAGS        ""
setenv CXXFLAGS      ""
setenv FFLAGS        ""
setenv F77FLAGS      ""
setenv F90FLAGS      ""
setenv LDFLAGS       ""

setenv KSL_CFLAGS    ""
setenv KSL_CXXFLAGS  ""
setenv KSL_FFLAGS    ""
setenv KSL_F77FLAGS  ""
setenv KSL_F90FLAGS  ""
setenv KSL_LDFLAGS   ""

# The below magic is for Emacs editing convenience
# Local Variables: 
# mode: tcl
# End:
# 